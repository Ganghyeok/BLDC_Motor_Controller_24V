#include "main.h"

int main(void)
{
	SystemClock_Config(SYSCLK_FREQ_72MHZ);

	RCC_MCOConfig(MCO_OPTION_SYSCLK);

	GPIOTest_Init();

	while(1)
	{
		if(GPIO_ReadPin(GPIOA, GPIO_PIN_0))
		{
			GPIO_WritePin(GPIOA, GPIO_PIN_1, GPIO_PIN_SET);
		}
		else
		{
			GPIO_WritePin(GPIOA, GPIO_PIN_1, GPIO_PIN_RESET);
		}
		Delay_ms(20);
	}
}
